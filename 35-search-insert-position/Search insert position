// This code complexity is O(N)

public class ProblemSolve {
    public static void main(String[] args) {
        int[] arr = {1,3,5,6};
        System.out.println(searchItem(arr,7));

    }

    static int searchItem(int[] arr, int tar){
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] >= tar){
                return i;
            }
        }
        return arr.length;
    }


}
