class Solution {
    public int[] maxSlidingWindow(int[] nums, int k) {
        int n = nums.length;
        int[] maxArr = new int[n-k+1];
        for(int i=0; i<=n-k; i++){
            int max = nums[i];
            for(int j = i; j < i + k; j++){
                if(nums[j] > max){
                    max = nums[j];
                }
            }
            maxArr[i] = max;
        }
        return maxArr;
    }
}
// In this question Time Limit Exceeded problem because more complexity O(n*k) of this code
